{
  "name": "@cryptlex/lexactivator",
  "version": "3.9.0",
  "description": "LexActivator API wrapper for Node.js.",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/node-pre-gyp rebuild",
    "build-publish": "./node_modules/.bin/node-pre-gyp rebuild package unpublish publish info",
    "deploy": "npm publish --access public",
    "preinstall": "cd bin && npm i && cd .. && node bin/preinstall.js",
    "install": "./node_modules/.bin/node-pre-gyp install --fallback-to-build"
  },
  "keywords": [
    "cryptlex",
    "lexactivator",
    "licensing library"
  ],
  "author": "Cryptlex, LLC",
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^1.6.3",
    "node-pre-gyp": "^0.14.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.653.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptlex/lexactivator-js.git"
  },
  "binary": {
    "module_name": "lexactivator",
    "module_path": "./lib/binding/{platform}-{arch}-napi-v{napi_build_version}-{libc}/",
    "host": "https://dl.cryptlex.com",
    "remote_path": "./downloads/nodejs/{module_name}/v{version}/",
    "package_name": "{module_name}-{platform}-{arch}-napi-v{napi_build_version}-{libc}.tar.gz",
    "napi_versions": [
      3
    ]
  },
  "homepage": "https://cryptlex.com",
  "gypfile": true
}
